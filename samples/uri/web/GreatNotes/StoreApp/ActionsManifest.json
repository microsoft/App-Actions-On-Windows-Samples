{
  "version": 2,
  "actions": [
    {
      "id": "AProtocol.OpenImage",
      "description": "Open an image on the Protocol app",
      "inputs": [
        {
          "name": "File",
          "kind": "Photo"
        }
      ],
      "outputs": [],
      "invocation": {
        "type": "Uri",
        "uri": "web+pwa://watchmovie"
      },
      "inputCombinations": [
        {
          "inputs": [ "File" ],
          "description": "Photo to open in the Protocol app"
        }
      ]
    },
    {
      "id": "Greatnotes.MakeNotes.File",
      "description": "Make notes in a file.",
      "inputs": [
        {
          "name": "File",
          "kind": "File"
        }
      ],
      "outputs": [],
      "instantiationDescription": "Make notes in a file.",
      "invocation": {
        "type": "Uri",
        "uri": "web+gnotes://makenotes"
      },
      "inputCombinations": [
        {
          "inputs": [ "File" ],
          "description": "File to be opened in Greatnotes."
        }
      ]
    },
    {
      "id": "Greatnotes.MakeNotes.Photo",
      "description": "Make notes in a photo.",
